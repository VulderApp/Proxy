{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Yarp": {
    "Routes": {
      "timetableRoute": {
        "ClusterId": "Timetable",
        "Match": {
          "Path": "/timetable/{**catch-all}"
        }
      },
      "branchRoute": {
        "ClusterId": "Timetable",
        "Match": {
          "Path": "/branch/{**catch-all}"
        }
      },
      "schoolRoute": {
        "ClusterId": "School",
        "Match": {
          "Path": "/school/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "Timetable": {
        "Destinations": {
          "Timetable": {
            "Address": "http://localhost:6000"
          },
          "Branch": {
            "Address": "http://localhost:6000"
          }
        }
      },
      "School": {
        "Destinations": {
          "School": {
            "Address": "http://localhost:6001"
          }
        }
      }
    }
  },
  "AllowedHosts": "*"
}
